import java.io.*;

public class FISE1{

    public static void main(String[]] args) throws IOException {
        
        FileInputStream fin = new FileInputStream("C:/Users/YASH/Downloads/FOSE1.txt");
        int i;
        while((i=fin.read()) != -1)
        {
            System.out.print((char)i);
        }
        fin.close();
    }

}
